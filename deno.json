{
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check",
    "build": "vite build",
    "preview": "deno serve -A _fresh/server.js",
    "dev": "vite",
    "update": "deno run -A -r jsr:@fresh/update ."
  },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "exclude": ["**/_fresh/*"],
  "imports": {
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^1.0.6",
    "@preact-icons/tb": "jsr:@preact-icons/tb@^1.0.14",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.16",
    "daisyui": "npm:daisyui@^5.3.10",
    "es-toolkit": "jsr:@es-toolkit/es-toolkit@^1.41.0",
    "@preact/signals": "npm:@preact/signals@^2.3.2",
    "preact": "npm:preact@^10.27.2",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.5",
    "appConfig": "./config.ts",
    "clsx": "npm:clsx@2.1.1",
    "components": "./components/index.ts",
    "components/shared": "./components/shared/index.ts",
    "components/Bills": "./components/Bills/index.ts",
    "components/ExecutiveOrders": "./components/ExecutiveOrders/index.ts",
    "context": "./islands/context/index.ts",
    "dayjs": "npm:dayjs@^1.11.18",
    "hooks": "./hooks/index.ts",
    "islands": "./islands/index.ts",
    "types": "./types/index.ts",
    "tailwindcss": "npm:tailwindcss@^4.1.16",
    "@tailwindcss/typography": "npm:@tailwindcss/typography@^0.5.19",
    "utils": "./utilities/index.ts",
    "fresh": "jsr:@fresh/core@^2.1.3",
    "vite": "npm:vite@^7.1.12"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": ["a", "img", "source", "body", "html", "head"]
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 2,
    "semiColons": true
  },
  "nodeModulesDir": "auto"
}
